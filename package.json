{
  "name": "panda-interview",
  "version": "0.0.1",
  "description": "Convenience wrapper for Prompt module interface",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "node_modules/.bin/coffee --compile --transpile --inline-map --output lib/ src/",
    "postpublish": "(json -f package.json version | xargs -I version git tag -am version version) && git push --tags",
    "test": "coffee --transpile test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pandastrike/panda-interview.git"
  },
  "keywords": [
    "prompt",
    "cli",
    "command",
    "line",
    "commandline"
  ],
  "author": "David Harper",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pandastrike/panda-interview/issues"
  },
  "homepage": "https://github.com/pandastrike/panda-interview#readme",
  "devDependencies": {
    "amen": "2.0.3",
    "babel-core": "6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "1.6.1",
    "coffeescript": "2.1.0",
    "json": "^9.0.6"
  },
  "dependencies": {
    "prompt": "1.0.0"
  }
}
